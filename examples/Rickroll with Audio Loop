DELAY 3000
GUI r
DELAY 500
STRING cmd
ENTER
DELAY 1000
STRING cd %temp%
ENTER
STRING echo Dim oPlayer > rick.vbs
ENTER
STRING echo Set oPlayer = CreateObject("WMPlayer.OCX") >> rick.vbs
ENTER
STRING echo oPlayer.URL = "https://www.youtube.com/watch?v=dQw4w9WgXcQ" >> rick.vbs
ENTER
STRING echo oPlayer.controls.play >> rick.vbs
ENTER
STRING echo While oPlayer.playState <> 1 : WScript.Sleep 100 : Wend >> rick.vbs
ENTER
STRING echo oPlayer.close >> rick.vbs
ENTER
STRING start rick.vbs
ENTER
REM Increase volume
STRING powershell $wsh = New-Object -ComObject WScript.Shell; while(1){$wsh.SendKeys([char]175); Start-Sleep -m 500}
ENTER   
